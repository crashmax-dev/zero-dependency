  "keywords": [
    "zero-dependency",
    "zero",
    "dependency",
    "utils",
    "dom",
    "storage",
    "cookie",
    "fetcher",
    "emitter",
    "interact",
    "react"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zero-dependency",
  "bugs": {
    "url": "https://github.com/zero-dependency/issues"
  },
  "author": {
    "name": "Vitalij Ryndin",
    "email": "sys@crashmax.ru",
    "url": "https://crashmax.ru"
  },
  "scripts": {
