    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest --ui --watch --coverage"
  },
  "devDependencies": {
    "@crashmax/tsconfig": "2.0.1",
    "@types/node": "18.15.11",
    "@vitest/coverage-c8": "0.30.1",
