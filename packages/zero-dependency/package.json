    "@zero-dependency/dom": "workspace:1.6.1",
    "@zero-dependency/emitter": "workspace:1.2.2",
    "@zero-dependency/fetcher": "workspace:1.2.2",
    "@zero-dependency/interact": "workspace:0.2.1",
    "@zero-dependency/react": "workspace:0.17.0",
    "@zero-dependency/storage": "workspace:1.3.1",
    "@zero-dependency/utils": "workspace:1.5.0"
  }
